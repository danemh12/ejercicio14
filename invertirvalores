#include<stdio.h>

main(){
	
	int x, a, b, c;
	
	printf("introduce el valor de a: \n" );
	scanf("%d", &a);
	printf("introduce el valor de x: \n" );
	scanf("%d", &b);
	printf("introduce el valor de b: \n" );
	scanf("%d", &x);
	c=a;
	a=b;
	b=x;
	x=c;
	printf("el resultado es: %d, %d, %d", a, b, x);
	
}
